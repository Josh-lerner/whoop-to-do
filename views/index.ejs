    <h3>My Lists</h3>
    <section class="display-lists" id="display-lists">
      <article class="list" id="list">
        <h5><%= list[1].title %></h5>
        <ul>
          <% let arr = [] %>
          <% for (let item in list) { %>
            <% if (!arr.includes(list[item].title)) { %>
              <% arr.push(list[item].title) %>
            <% } %>
          <% } %>
          <% for (let item of arr) { %>
            <li><%= item %></li>
          <% } %>
        </ul>

      </article>


